struct Pessoa {
    int idade;
    int peso;
};

Pessoa p;
int notas[3];
int i;

void main() {
    p.idade = 30;
    p.peso  = 80;

    for(i=0;i<10;i++)
		notas[i] = i;
	
	
    write("idade = ", p.idade);
    write("peso = ", p.peso);
    write("notas[0] = ", notas[0]);
    write("notas[1] = ", notas[1]);
    write("notas[9] = ", notas[9]);
}