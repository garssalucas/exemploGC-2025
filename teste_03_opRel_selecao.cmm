struct Ponto {
    int x;
    int y;
	float f;
	bool b;
};

Ponto p;
int a;

void main() {
	a=100;
    p.x = 10;
    p.y = 20;
	p.f = 0-100;
	p.b = --a;
    write("X = ", p.x);
    write("Y = ", p.y);
	write("F = ", p.f);
    write("B = ", p.b);
	a = p.x;
	p.b = a--;
    write("B = ", p.b);
	write("A = ", a);
}